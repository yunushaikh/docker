# MariaDB Galera Cluster Configuration
# You can modify these values to customize your setup

# MariaDB Version (e.g., 11.4, 11.3, 11.2, 11.1, 11.0, 10.11, 10.10, 10.9, 10.8, 10.7, 10.6, 10.5, 10.4, 10.3, 10.2, 10.1)
MARIADB_VERSION=10.11

# Database Configuration
MYSQL_ROOT_PASSWORD=rootpassword
MYSQL_DATABASE=testdb
MYSQL_USER=testuser
MYSQL_PASSWORD=testpass

# Galera Cluster Configuration
GALERA_CLUSTER_NAME=galera-cluster
GALERA_SST_METHOD=rsync

# ProxySQL Configuration
PROXYSQL_ADMIN_USER=admin
PROXYSQL_ADMIN_PASSWORD=admin

# Network Configuration
NETWORK_SUBNET=172.20.0.0/16

# Resource Configuration
INNODB_BUFFER_POOL_SIZE=128M
MAX_CONNECTIONS=1000
